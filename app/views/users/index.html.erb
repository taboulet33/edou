<h1>Listing users</h1>

<table>
  <tr>
    <th>Nom</th>
    <th>Prenom</th>
    <th>Mail</th>
    <th>Professionnel</th>
    <th>Resume</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.nom %></td>
    <td><%= user.prenom %></td>
    <td><%= user.mail %></td>
    <td><%= user.professionnel %></td>
    <td><%= user.resume %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

